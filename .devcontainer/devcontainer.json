{
  "name": "fastgtp-dev",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "fastgtp",
  "workspaceFolder": "/workspace",
  "initializeCommand": "bash -lc 'if [ ! -f .env ] && [ -f .env.sample ]; then cp .env.sample .env; fi'",
  "remoteUser": "vscode",
  "postCreateCommand": "bash -lc 'cd /workspace && if [ -f pyproject.toml ]; then poetry install --no-root; fi'",
  "postStartCommand": "bash -lc 'cd /workspace && if [ -f pyproject.toml ] && [ ! -d .venv ]; then poetry install --no-root; fi'",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "streetsidesoftware.code-spell-checker",
        "njpwerner.autodocstring",
        "ms-python.black-formatter",
        "ms-python.isort",
        "mhutchie.git-graph"
      ],
      "settings": {
        "python.pythonPath": ".venv/bin/python",
        "python.formatting.provider": "black",
        "python.linting.enabled": true,
        "python.linting.mypyEnabled": true,
        "terminal.integrated.defaultProfile.linux": "bash",
        "files.exclude": {
          "**/__pycache__": true,
          "**/.pytest_cache": true,
          "**/.mypy_cache": true
        },
        "cSpell.words": [
          "fastgtp",
          "boardsize"
        ],
        "editor.tabSize": 4
      }
    }
  },
  "overrideCommand": true,
  "runServices": [
    "fastgtp"
  ],
  "shutdownAction": "stopCompose"
}